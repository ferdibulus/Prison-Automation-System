@model IEnumerable<jailOtomationSystem.Models.prisonerCell>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="card">
    <div class="card-body">


        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.capacity)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.cellType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.area)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.availableCount)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.capacity)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.cellType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.area)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.availableCount)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.prisonerCellID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.prisonerCellID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.prisonerCellID })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>